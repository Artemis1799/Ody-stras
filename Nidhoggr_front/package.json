{
  "name": "nidhoggr-front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "ws-server": "node websocket-server.js",
    "start:full": "concurrently \"npm run ws-server\" \"npm run start\"",
    "extract-tiles": "node tools/extract_tiles.js",
    "build": "npm run lint && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:once": "ng test --watch=false --browsers=ChromeHeadless",
    "test:coverage": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "test:coverage:open": "ng test --watch=false --code-coverage && open coverage/index.html",
    "test:watch": "ng test --watch=true",
    "test:debug": "ng test --browsers=Chrome --watch=true --source-map",
    "lint": "ng lint --max-warnings=10",
    "lint:fix": "ng lint --fix",
    "serve:ssr:Nidhoggr_front": "node dist/Nidhoggr_front/server/server.mjs"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.13",
    "@angular/cdk": "^20.2.14",
    "@angular/common": "^20.3.13",
    "@angular/compiler": "^20.3.13",
    "@angular/core": "^20.3.13",
    "@angular/forms": "^20.3.0",
    "@angular/material": "^20.2.14",
    "@angular/platform-browser": "^20.3.13",
    "@angular/platform-browser-dynamic": "^20.3.13",
    "@angular/platform-server": "^20.3.0",
    "@angular/router": "^20.3.13",
    "@angular/ssr": "^20.3.9",
    "@primeng/themes": "^21.0.1",
    "@worktile/gantt": "^20.0.0",
    "better-sqlite3": "^12.4.1",
    "express": "^5.1.0",
    "jspdf": "^3.0.4",
    "leaflet": "^1.9.4",
    "leaflet-draw": "^1.0.4",
    "maplibre-gl": "^5.13.0",
    "primeicons": "^7.0.0",
    "primeng": "^20.3.0",
    "qrcode": "^1.5.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "ws": "^8.18.3",
    "xlsx": "^0.18.5",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^21.0.1",
    "@angular-eslint/eslint-plugin": "^21.0.1",
    "@angular-eslint/eslint-plugin-template": "^21.0.1",
    "@angular-eslint/schematics": "^21.0.1",
    "@angular/build": "^20.3.9",
    "@angular/cli": "^20.3.9",
    "@angular/compiler-cli": "^20.3.0",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/leaflet": "^1.9.21",
    "@types/leaflet-draw": "^1.0.13",
    "@types/maplibre-gl": "^1.13.2",
    "@types/node": "^20.17.19",
    "@types/qrcode": "^1.5.6",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^61.4.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "karma-junit-reporter": "~2.0.1",
    "karma-spec-reporter": "^0.0.36",
    "typescript": "~5.9.2"
  }
}
